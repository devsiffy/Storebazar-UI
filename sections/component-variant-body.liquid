{% assign all_components =  page.metafields.custom.all_components.value %}
{% assign all_components_size =  page.metafields.custom.all_components.value | size %}


{% if all_components_size <= 1 %}
  {{ all_components | json }}
{% endif %}
{% comment %} 
{% for component in all_components %}
  {% assign component_variant = component.component_variants.value %}
  {{ component_variant.title }}
{% endfor %} {% endcomment %}
